<?xml version="1.0" encoding="UTF-8"?>
<webapp>
	<section type="project">
		<title>Project</title>
		<project_name attribut="input">Ads Manager</project_name>
		<project_author attribut="input">Olivier Gaillard</project_author>
	</section>
	<section type="connection">
		<title>Connection</title>
		<db_server attribut="input">localhost</db_server>
		<db_user attribut="input">ads_manager</db_user>
		<db_password attribut="input">ads_manager</db_password>
	</section>
	<section type="database">
		<title>Database</title>
		<db_base attribut="select">ads_manager</db_base>
	</section>
	<section type="table">
		<title>Table</title>
		<table attribut="select">ads</table>
		<objet attribut="input">ad</objet>
		<objets attribut="input">ads</objets>
	</section>
	<section type="fields">
		<title>Fields</title>
		<fields attribut="field">id,key,creation_date,title,slug,description,datasheet,stock,payment,photo_path,price,author_id,phone_number,hide_phone,email,password,signal,is_top,ad_category_id,nb_views,state</fields>

		<objet2 attribut="input"/>
		<objet3 attribut="input"/>
	</section>
</webapp>
