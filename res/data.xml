<?xml version="1.0" encoding="UTF-8"?>
<webapp>
	<section type="project">
		<title>Project</title>
		<project_name attribut="input">test</project_name>
		<project_author attribut="input">Olivier Gaillard</project_author>
	</section>
	<section type="connection">
		<title>Connection</title>
		<db_server attribut="input">localhost</db_server>
		<db_user attribut="input">memo_info</db_user>
		<db_password attribut="input">memo_info</db_password>
	</section>
	<section type="database">
		<title>Database</title>
		<db_base attribut="select">test</db_base>
	</section>
	<section type="table">
		<title>Table</title>
		<table attribut="select">peoples</table>
		<objet attribut="input">people</objet>
		<objets attribut="input">peoples</objets>
	</section>
	<section type="fields">
		<title>Fields</title>
		<fields attribut="field">id,team_id,firstname,lastname,birthdate,address,postcode,city</fields>

		<linked_objet attribut="input"/>
		<linked_objets attribut="input"/>
		<search_engine attribut="checkbox"/>
		<authentification attribut="checkbox"/>
	</section>
</webapp>
